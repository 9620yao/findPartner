<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.ssm.mapper.PartnerMapper">
	<select id="fingPartner" parameterType="Partner" resultType="Partner">
		select * from logining where password=#{password} and (email=#{email} or phone=#{email})
	</select>
	
	<insert id="insertPar" parameterType="Partner">
		insert into logining(lid,email,password,regdate,uname) values('a'||seq_logining_lid.nextval,#{email},#{password},to_char(sysdate,'yyyy-MM-dd'),'a'||seq_logining_lid.currval)
	</insert>
	
	<select id="fingEmail" parameterType="Partner" resultType="Partner">
		select * from logining where email=#{email}
	</select>
</mapper>